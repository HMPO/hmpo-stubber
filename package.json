{
  "name": "hmpo-stubber",
  "version": "1.0.1",
  "description": "Create dynamic stub API services using a simple JSON configuration",
  "main": "./lib",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:unit && npm run test:cover",
    "test:lint": "eslint .",
    "test:unit": "mocha test/unit/ --recursive --require test/unit/helper",
    "test:cover": "istanbul cover _mocha -- -R dot test/unit/ --recursive --require test/unit/helper"
  },
  "bin": {
    "hmpo-stubber": "./bin/stubber.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/passports-stubber.git"
  },
  "keywords": [
    "stub",
    "mock",
    "api",
    "json"
  ],
  "author": "HMPO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/passports-stubber/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/passports-stubber#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "proxyquire": "^1.8.0",
    "reqres": "^1.3.0",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "optionator": "^0.8.2",
    "underscore": "^1.8.3"
  }
}
